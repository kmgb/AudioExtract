{
  "name": "AudioExtract",
  "version": "1.0",
  "description": "Extract audio from tabs and save as a file.",
  "permissions": [
    "tabCapture"
  ],
  "browser_action": {
      "default_title": "AudioExtract",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
